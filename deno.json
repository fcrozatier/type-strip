{
  "version": "1.0.1",
  "name": "@fcrozatier/type-strip",
  "license": "MIT",
  "exports": "./index.ts",
  "tasks": {
    "gen:fixtures": "deno run --allow-write scripts/fixtures-script.ts",
    "bench": "deno bench --allow-read --allow-env --allow-run scripts/bench.ts ",
    "test": "deno test --allow-read",
    "coverage": "deno test --allow-read --coverage && deno coverage --include='typestrip.ts' --html && open coverage/html/index.html"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/fs": "jsr:@std/fs@^1.0.7",
    "@std/path": "jsr:@std/path@^1.0.8",
    "esbuild": "npm:esbuild@^0.24.0",
    "ts-blank-space": "npm:ts-blank-space@^0.4.4",
    "typescript": "npm:typescript@^5.7.2"
  },
  "exclude": ["tests/**/input.ts", "tests/**/input.tsx", "tests/**/output.ts"],
  "publish": { "exclude": ["tests", "scripts", ".vscode"] }
}
